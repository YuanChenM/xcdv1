<?xml version="1.0" encoding="utf-8"?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee web-jsptaglibrary_2_0.xsd"
        version="2.0">
    <tlib-version>1.0</tlib-version>
    <short-name>msk-district</short-name>
    <uri>/district</uri>
    <tag>
        <name>input</name>
        <tag-class>com.msk.core.web.tag.SelectDistrictTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
            <name>selectType</name>
            <required>true</required>
            <fragment>true</fragment>
        </attribute>
        <attribute>
            <name>selectAreaName</name>
            <required>true</required>
            <fragment>true</fragment>
        </attribute>
        <attribute>
            <name>selectAreaId</name>
            <required>true</required>
            <fragment>true</fragment>
        </attribute>
        <attribute>
            <name>selectCityName</name>
            <required>false</required>
            <fragment>true</fragment>
        </attribute>
        <attribute>
            <name>selectCityId</name>
            <required>false</required>
            <fragment>true</fragment>
        </attribute>
        <attribute>
            <name>selectDistrictId</name>
            <required>false</required>
            <fragment>true</fragment>
        </attribute>
        <attribute>
            <name>selectDistrictName</name>
            <required>false</required>
            <fragment>true</fragment>
        </attribute>
    </tag>
</taglib>
