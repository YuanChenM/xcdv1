<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="com.msk.pd.logic.IPD141142Logic">
    <select id="count"  resultType="int">
         SELECT
            COUNT(1)
            FROM pd_standard
    </select>


    <select id="getPageCount" parameterType="com.msk.pd.bean.IPD141142RsParam" resultType="int">
       SELECT
            COUNT(1)
            FROM pd_standard
    </select>

    <select id="findPageList" parameterType="com.msk.pd.bean.IPD141142RsParam" resultType="com.msk.pd.bean.IPD141142RsBean">
        SELECT
        A.STANDARD_ID AS standardId
        ,NULLIF(A.CLASSES_CODE,'') AS classesCode
        ,NULLIF(A.BREED_CODE,'') AS  breedCode
        ,NULLIF(A.MACHINING_CODE,'') AS machiningCode
        ,NULLIF(A.FEATURE_CODE,'') AS featureCode
        ,NULLIF(A.WEIGHT_CODE,'') AS weightCode
        FROM PD_STANDARD A
        <if test="startPos>=0 and pageCount>0">
            LIMIT #{startPos}, #{pageCount}
        </if>
    </select>
</mapper>







